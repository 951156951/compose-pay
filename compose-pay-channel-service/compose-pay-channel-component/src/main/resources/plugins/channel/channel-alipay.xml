<plugin-channel>
    <channel>alipay</channel>
    <service>

        <!-- 当面付扫码支付 -->
        <bean id="scan_code" class="io.github.pleuvoir.channel.channels.alipay.AlipayScanCode"/>
        <!-- APP支付 -->
        <bean id="app" class="io.github.pleuvoir.channel.channels.alipay.AlipayApp"/>
        <!-- 手机网站支付 -->
        <bean id="mobile_h5" class="io.github.pleuvoir.channel.channels.alipay.AlipayMobileH5"/>
        <!-- 电脑网站支付 -->
        <bean id="mobile_h5" class="io.github.pleuvoir.channel.channels.alipay.AlipayPcWeb"/>
        <!-- 支付结果查询 -->
        <bean id="pay_query" class="io.github.pleuvoir.channel.channels.alipay.AlipayPayQuery"/>


    </service>

</plugin-channel>